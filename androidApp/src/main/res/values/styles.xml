<resources>
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorSecondary">@color/design_default_color_secondary</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Text_Regular_Black_15" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="Text_Bold_Gray800_36" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/sf_pro_display_bold</item>
        <item name="android:textColor">@color/gray_800</item>
        <item name="android:textSize">36sp</item>
    </style>

    <style name="Text_Regular_Gray50_13" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textColor">@color/gray_50</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="Text_DupletBold_40_Prune900" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/duplet_open_bold</item>
        <item name="android:textColor">@color/prune_900</item>
        <item name="android:textSize">40sp</item>
    </style>

    <style name="Text_SfSemiBold_17_Prune700" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/sf_pro_text_semibold</item>
        <item name="android:textColor">@color/prune_700</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="Text_DupletBold_36_Prune800">
        <item name="fontFamily">@font/duplet_open_bold</item>
        <item name="android:textColor">@color/prune_800</item>
        <item name="android:textSize">36sp</item>
    </style>

    <style name="Text_SfRegular_15_Prune500">
        <item name="fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textColor">@color/prune_500</item>
        <item name="android:textSize">15sp</item>
    </style>
</resources>